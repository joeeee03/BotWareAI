{
  "build": {
    "builder": "dockerfile",
    "dockerfilePath": "Dockerfile"
  },
  "deploy": {
    "restartPolicyType": "on_failure",
    "restartPolicyMaxRetries": 5,
    "healthcheckPath": "/",
    "healthcheckInterval": 30,
    "healthcheckTimeout": 15,
    "healthcheckStartPeriod": 120
  }
}
